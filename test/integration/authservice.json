{
  "listen_address": "0.0.0.0",
  "listen_port": "10003",
  "log_level": "trace",
  "threads": 8,
  "chains": [
    {
      "name": "idp_filter_chain",
      "filters": [
      {
        "oidc":
          {
            "authorization_uri": "https://idp:8443/auth/realms/master/protocol/openid-connect/auth",
            "token_uri": "https://idp:8443/auth/realms/master/protocol/openid-connect/token",
            "client_id": "authservice",
            "client_secret": "secret",
            "callback_uri": "https://localhost:9000/oauth/callback",
            "jwks_fetcher": {
              "jwks_uri": "https://idp:8443/auth/realms/master/protocol/openid-connect/certs"
            },
            "scopes": [],
            "id_token": {
              "preamble": "Bearer",
              "header": "Authorization"
            },
            "logout": {
              "path": "/logout",
              "redirect_uri": "https://google.com"
            }
          }
        }
      ]
    }
  ]
}
